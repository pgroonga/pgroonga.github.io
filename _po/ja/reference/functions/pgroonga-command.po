"--      [                                                     +\n"
"--          [                                                 +\n"
"--              [                                             +\n"
"--                  1                                         +\n"
"--              ],                                            +\n"
"--              [                                             +\n"
"--                  [                                         +\n"
"--                      \"_id\",                                +\n"
"--                      \"UInt32\"                              +\n"
"--                  ],                                        +\n"
"--                  [                                         +\n"
"--                      \"content\",                            +\n"
"--                      \"LongText\"                            +\n"
"--                  ],                                        +\n"
"--                  [                                         +\n"
