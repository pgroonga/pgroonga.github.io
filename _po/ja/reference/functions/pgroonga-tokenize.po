"```sql\n"
"SELECT jsonb_pretty(token::jsonb)\n"
"  FROM (\n"
"    SELECT unnest(pgroonga_tokenize('aBcDe 123',\n"
"                                    'tokenizer', 'TokenNgram'))\n"
"      AS token\n"
"  ) AS tokens;\n"
"--            jsonb_pretty           \n"
"-- ----------------------------------\n"
"--  {                               +\n"
"--      \"value\": \"aB\",              +\n"
"--      \"position\": 0,              +\n"
"--      \"force_prefix_search\": false+\n"
"--  }\n"
"--  {                               +\n"
"--      \"value\": \"Bc\",              +\n"
"--      \"position\": 1,              +\n"
"--      \"force_prefix_search\": true +\n"
"--  }\n"
"--  {                               +\n"
"--      \"value\": \"cD\",              +\n"
"--      \"position\": 2,              +\n"
"--      \"force_prefix_search\": true +\n"
"--  }\n"
"--  {                               +\n"
"--      \"value\": \"De\",              +\n"
"--      \"position\": 3,              +\n"
"--      \"force_prefix_search\": true +\n"
"--  }\n"
"--  {                               +\n"
